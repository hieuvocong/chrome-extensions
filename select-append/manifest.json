{
  "manifest_version": 2,
  "name": "Select Append",
  "description": "Append option to select tag.",
  "version": "0.4",
  "icons": {
    "128": "icon-128x128.png"
  },
  "permissions": ["activeTab"],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "all_frames": true,
      "js": ["content.js"]
    }
  ]
}
